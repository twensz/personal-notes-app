{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "linebreak-style": "off",
        "react/prefer-stateless-function": "off",
        "react/jsx-filename-extension": "off",
        "react/forbid-prop-types": "off",
        "react/jsx-no-bind": "off"
    }
}
